{
  "actorSpecification": 1,
  "name": "googletrend-west",
  "title": "Google Trends (West) via Pytrends",
  "version": "0.0",
  "buildTag": "latest",
  "dockerfile": "./Dockerfile",
  "input": {
    "schemaVersion": 1,
    "title": "Input",
    "type": "object",
    "properties": {
      "keywords": {
        "type": "array",
        "title": "Keywords (list)",
        "items": { "type": "string" },
        "default": ["메디큐브", "코스알엑스", "아누아", "조선미녀", "K-SECRET", "ARENCIA", "MIXSOON"]
      },
      "timeframe": {
        "type": "string",
        "title": "Timeframe",
        "description": "Ví dụ: '2023-08-01 2023-08-31' hoặc 'today 1-m', 'today 12-m'",
        "default": "2023-08-01 2023-08-31"
      },
      "geo": {
        "type": "string",
        "title": "Country (geo)",
        "default": "US"
      },
      "gprop": {
        "type": "string",
        "title": "Google property",
        "enum": ["web", "images", "news", "youtube", "froogle"],
        "default": "web"
      }
    },
    "required": ["keywords"]
  }
}
